{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "jwt": {
      "type": "library",
      "root": "libs/jwt",
      "entryFile": "index",
      "sourceRoot": "libs/jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/jwt/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "email": {
      "type": "library",
      "root": "libs/email",
      "entryFile": "index",
      "sourceRoot": "libs/email/src",
      "compilerOptions": {
        "tsConfigPath": "libs/email/tsconfig.lib.json"
      }
    },
    "payment": {
      "type": "library",
      "root": "libs/payment",
      "entryFile": "index",
      "sourceRoot": "libs/payment/src",
      "compilerOptions": {
        "tsConfigPath": "libs/payment/tsconfig.lib.json"
      }
    },
    "openai": {
      "type": "library",
      "root": "libs/openai",
      "entryFile": "index",
      "sourceRoot": "libs/openai/src",
      "compilerOptions": {
        "tsConfigPath": "libs/openai/tsconfig.lib.json"
      }
    },
    "key-pool": {
      "type": "library",
      "root": "libs/key-pool",
      "entryFile": "index",
      "sourceRoot": "libs/key-pool/src",
      "compilerOptions": {
        "tsConfigPath": "libs/key-pool/tsconfig.lib.json"
      }
    },
    "content-safe": {
      "type": "library",
      "root": "libs/content-safe",
      "entryFile": "index",
      "sourceRoot": "libs/content-safe/src",
      "compilerOptions": {
        "tsConfigPath": "libs/content-safe/tsconfig.lib.json"
      }
    }
  }
}