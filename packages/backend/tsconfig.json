{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/prisma/*": [
        "prisma/*"
      ],
      "@libs/*": [
        "libs/*"
      ],
      "@libs/jwt": [
        "libs/jwt/src"
      ],
      "@libs/jwt/*": [
        "libs/jwt/src/*"
      ],
      "@libs/redis": [
        "libs/redis/src"
      ],
      "@libs/redis/*": [
        "libs/redis/src/*"
      ],
      "@libs/email": [
        "libs/email/src"
      ],
      "@libs/email/*": [
        "libs/email/src/*"
      ],
      "@libs/payment": [
        "libs/payment/src"
      ],
      "@libs/payment/*": [
        "libs/payment/src/*"
      ],
      "@libs/openai": [
        "libs/openai/src"
      ],
      "@libs/openai/*": [
        "libs/openai/src/*"
      ],
      "@libs/key-pool": [
        "libs/key-pool/src"
      ],
      "@libs/key-pool/*": [
        "libs/key-pool/src/*"
      ],
      "@libs/content-safe": [
        "libs/content-safe/src"
      ],
      "@libs/content-safe/*": [
        "libs/content-safe/src/*"
      ]
    },
    "types": [
      "@types/caw"
    ]
  },
  "incremental": true,
  "skipLibCheck": true,
  "strictNullChecks": false,
  "noImplicitAny": false,
  "strictBindCallApply": false,
  "forceConsistentCasingInFileNames": false,
  "noFallthroughCasesInSwitch": false
}